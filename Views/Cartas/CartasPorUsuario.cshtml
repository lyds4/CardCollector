@model IQueryable<CardCollector.Models.ViewModel.CardUserViewModel>;
<table class="table">
    <thead>
        <tr>
            <th>
                <!--Html.DisplayNameFor(model => model.Employee.First_Name) -->
                Card Name
            </th>
            <th>
                <!--Html.DisplayNameFor(model => model.Employee.Last_Name) -->
                Description
            </th>
            <th>
                <!--Html.DisplayNameFor(model => model.Employee.Dni) -->
                Owner
            </th>
            <th>
                <!--Html.DisplayNameFor(model => model.Employee.Dni) -->
                Design
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cardName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.userName)
            </td>
            <td>
                ImageUser@item.idCard
            </td>

            <td>
                <a asp-controller="Card" asp-action="GifCard" asp-route-id="@item.idCard">Regalar</a>
            </td>
        </tr>
        }
    </tbody>
</table>